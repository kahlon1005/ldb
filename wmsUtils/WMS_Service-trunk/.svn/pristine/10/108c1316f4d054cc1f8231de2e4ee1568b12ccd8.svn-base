package com.bcldb.util;

import javax.ejb.EJBException;

public class WMSUtilServiceException extends EJBException {
    
	private static final long serialVersionUID = 6328439798350899856L;
    
    public WMSUtilServiceException(String string, Exception exception) {
        super(string, exception);
    }

    public WMSUtilServiceException(Exception exception) {
        super(exception);
    }

    public WMSUtilServiceException(String string) {
        super(string);
    }

    public WMSUtilServiceException() {
        super();
    }
}
