<div class="container">
    
    <div *ngIf="!success" class="alert alert-warning" role="alert">
        <strong>Error! </strong>{{error}}
    </div>    
    <div *ngIf="success" class="alert alert-success" role="alert">
        {{message}}
    </div>
    
    <button (click) = "goToTransactionList()" class="btn btn-secondary">Return</button>    

</div>

