			<XMLAttributes><XMLAttribute output="no" name="RS_CreateExtendedDataItems" value="false"/><XMLAttribute output="no" name="listSeparator" value=","/><XMLAttribute output="no" name="RS_modelModificationTime" value="2019-11-18T19:53:25.830Z"/></XMLAttributes><reportName>LDB Order Demand Report</reportName><queries><query name="Warehouse">
			<source>
				<model/>
			</source>
			<selection><dataItem name="Warehouse" label="Warehouse"><expression>[wms].[so_f].[whse_code]</expression></dataItem></selection>
		</query><query name="DataQuery">
			<source>
				
			<sqlQuery name="SQL" dataSource="wms">
			<sqlText>SELECT q.whse_code, 
q.sku, q.sku_desc, /*q.ship_date,*/ q.orders_qty, q.available, 
iif(q.available - q.orders_qty &lt; 0, q.available - q.orders_qty,0) unavailable, 
iif(q.available - q.orders_qty &lt; 0, &apos;Yes&apos;,&apos;No&apos;) replen_required, 
iif((q.available - q.orders_qty) *-1 &gt; total_max_tp_cap, &apos;Yes&apos;,&apos;No&apos;) new_fp_required, 
q.total_max_tp_cap, 
q.loc, (q.available_location/q.btl_per_case) as available_location, (trig_batch_qty* q.trig_uom_qty/q.btl_per_case) trig_batch_cases, 
iif((q.available_location) &gt; q.trig_batch_btl, &apos;No&apos;, &apos;Yes&apos;) generate_batch_replen,
(repl_batch_qty* q.rep_uom_qty/q.btl_per_case) repl_batch_cases, q.max_fp_cap

FROM (
	SELECT om_f.whse_code, pm_f.sku, pm_f.sku_desc, /*om_f.ship_date, */pm_f.custom_numeric_3 btl_per_case,
		sum(plan_qty - od_f.sched_qty - od_f.cmp_qty)/pm_f.custom_numeric_3 orders_qty, 

		COALESCE((SELECT sum(iv_f.qty + iv_f.intran_qty - iv_f.alloc_qty) 
			FROM iv_f 
			JOIN lc_f ON lc_f.whse_code = iv_f.whse_code and lc_f.loc = iv_f.loc and lc_f.loc_type = &apos;FP&apos;
			WHERE iv_f.whse_code = om_f.whse_code and iv_f.sku = pm_f.sku)/pm_f.custom_numeric_3,0) available, 

		(SELECT sum(lc_f.max_fp_cap) FROM lc_f WHERE lc_f.whse_code = om_f.whse_code and lc_f.sku = pm_f.sku) total_max_tp_cap,

               COALESCE((SELECT sum(iv_f.qty + iv_f.intran_qty - iv_f.alloc_qty) FROM iv_f WHERE iv_f.whse_code = om_f.whse_code AND iv_f.sku = pm_f.sku and iv_f.loc = lc_f.loc),0) available_location, 

		lc_f.loc, lc_f.trig_batch_qty, lc_f.repl_batch_qty, lc_f.max_fp_cap,
		(lc_f.trig_batch_qty * triguom.qty_uom_in) trig_batch_btl, triguom.qty_uom_in trig_uom_qty, repluom.qty_uom_in rep_uom_qty
	FROM om_f
	JOIN od_f ON od_f.whse_code = om_f.whse_code AND od_f.ob_oid = om_f.ob_oid
	JOIN pm_f ON pm_f.whse_code = om_f.whse_code AND pm_f.sku = od_f.sku
	JOIN lc_f ON lc_f.whse_code = om_f.whse_code AND lc_f.sku = od_f.sku
	JOIN v_u_pm_f_uom triguom ON triguom.whse_code = pm_f.whse_code and triguom.sku = pm_f.sku and triguom.pkg = pm_f.pkg and triguom.uom = lc_f.trig_uom
	JOIN v_u_pm_f_uom repluom ON repluom.whse_code = pm_f.whse_code and repluom.sku = pm_f.sku and repluom.pkg = pm_f.pkg and repluom.uom = lc_f.repl_uom
	WHERE lc_f.loc_type = &apos;FP&apos;
        AND om_f.ship_date BETWEEN  #prompt(&apos;ShipDateFrom&apos;)# AND  #prompt(&apos;ShipDateTo&apos;)#
	GROUP BY om_f.whse_code, pm_f.sku, pm_f.sku_desc, /*om_f.ship_date,*/ lc_f.loc, lc_f.trig_batch_qty, lc_f.trig_uom, lc_f.repl_batch_qty, lc_f.repl_uom,
	 lc_f.max_fp_cap ,pm_f.custom_numeric_3,triguom.qty_uom_in, repluom.qty_uom_in
) q</sqlText>
