			<XMLAttributes><XMLAttribute name="RS_CreateExtendedDataItems" output="no" value="true"/><XMLAttribute name="listSeparator" output="no" value=","/><XMLAttribute name="RS_modelModificationTime" output="no" value="2019-11-18T19:53:25.830Z"/></XMLAttributes><queries><query name="WMS">
			<source>
				
			<sqlQuery dataSource="wms" name="SQL1">
			<sqlText>SELECT a.[Warehouse]
      , a.[PO Number]
	  , a.[PO Line Number]
	  , a.[Receipt]
	  , a.[SKU]
	  , a.[Ordered Quantity (bottles)]	AS  &quot;Ordered Quantity (bottles)&quot;
	  , a.[Quantity Received (bottles)] AS &quot;Quantity Received (bottles)&quot;  
	  , CASE WHEN pm_f.uom1=&apos;BTL&apos; THEN a.[Ordered Quantity (bottles)]/ISNULL(pm_f.qty1in2, 1) ELSE a.[Ordered Quantity (bottles)] END AS &quot;Ordered Quantity (cases)&quot;
	  , CASE WHEN pm_f.uom1=&apos;BTL&apos; THEN a.[Quantity Received (bottles)]/ISNULL(pm_f.qty1in2, 1) ELSE a.[Quantity Received (bottles)] END AS &quot;Quantity Received (cases)&quot;
	  , CASE WHEN pm_f.uom1=&apos;BTL&apos; THEN a.[Quantity to be Received (bottles)]/ISNULL(pm_f.qty1in2, 1) ELSE a.[Quantity to be Received (bottles)] END AS &quot;Quantity to be Received (cases)&quot;
	  , a.[Order Creation Date]
	  , a.[Receipt Date]
	  , a.[Receipt Closeout Date]
	  , DATEDIFF(DAY, a.[Receipt Date], ISNULL(a.[Receipt Closeout Date], getDate())) AS &quot;# of Days Since Received&quot;
FROM (
SELECT  ibod_f.whse_code AS &quot;Warehouse&quot;
      , ibod_f.ib_oid AS &quot;PO Number&quot;
	  , ibod_f.ib_lno AS &quot;PO Line Number&quot;
	  , ibod_f.receipt AS &quot;Receipt&quot;
	  , ibod_f.sku AS &quot;SKU&quot;
	  , ibod_f.ord_qty AS &quot;Ordered Quantity (bottles)&quot;
	  , ibod_f.cmp_qty AS &quot;Quantity Received (bottles)&quot;
	  , ibod_f.ord_qty - cmp_qty AS &quot;Quantity to be Received (bottles)&quot;
	  , ibod_f.create_stamp AS &quot;Order Creation Date&quot;
	  , MIN(it_f.create_stamp) AS &quot;Receipt Date&quot;
	  , upr.create_stamp AS &quot;Receipt Closeout Date&quot;
 FROM   dbo.ibod_f
 LEFT OUTER JOIN dbo.it_f
   ON   ibod_f.whse_code = it_f.whse_code
  AND	ibod_f.receipt = it_f.receipt
  AND   ibod_f.ib_oid = it_f.ib_oid
  AND   ibod_f.ib_type = it_f.ib_type
  AND   ibod_f.ib_lno = it_f.ib_lno
  AND   it_f.transact = &apos;RCPT&apos;
LEFT OUTER JOIN dbo.v_u_up_rcpt upr
   ON   ibod_f.whse_code = upr.whse_code
  AND	ibod_f.receipt = upr.receipt
  AND   ibod_f.ib_oid = upr.ib_oid
  AND   ibod_f.ib_type = upr.ib_type
WHERE   ISNUMERIC(ibod_f.ib_oid) = 1
  AND   ibod_f.cmp_qty &gt; 0
  AND   ibod_f.receipt &lt;&gt; &apos;&apos;  
  AND (upr.up_rcpt_id is NULL OR upr.create_stamp &gt;= DATEADD(day, 1, #prompt(&apos;p_cutoffdate&apos;)#))  
GROUP BY ibod_f.whse_code, ibod_f.ib_oid, ibod_f.ib_lno, ibod_f.ib_type, ibod_f.receipt, ibod_f.sku, ibod_f.pkg, ibod_f.ord_qty, ibod_f.cmp_qty, ibod_f.create_stamp, upr.create_stamp
HAVING MIN(it_f.create_stamp) &lt; DATEADD(day, 1, #prompt(&apos;p_cutoffdate&apos;)#)
) AS a
JOIN dbo.pm_f
	ON	a.warehouse = pm_f.whse_code AND a.sku = pm_f.sku
ORDER BY DATEDIFF(DAY, a.[Receipt Date], ISNULL(a.[Receipt Closeout Date], getDate())) DESC

