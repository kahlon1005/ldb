</dataItem><dataItem name="eBiz Order Line Status" label="eBiz Order Line Status" aggregate="none" rollupAggregate="none"><expression>[eBiz with Cost].[Order Line Status]</expression></dataItem><dataItem name="eBiz Creation Date/Time" label="eBiz Creation Date/Time" aggregate="none" rollupAggregate="none"><expression>[eBiz with Cost].[Creation Date/Time]</expression></dataItem><dataItem name="eBiz Last Update Date/Time" label="eBiz Last Update Date/Time" aggregate="none" rollupAggregate="none"><expression>[eBiz with Cost].[Last Update Date/Time]</expression></dataItem><dataItem name="Cost per Shipping Unit" label="Cost per Case" aggregate="none" rollupAggregate="none"><expression>[eBiz with Cost].[PRODUCT_COST_PER_SHIP_UNIT]</expression></dataItem><dataItem name="Total Cost" label="Total Cost" aggregate="total" rollupAggregate="total"><expression>[WMS Case Quantity] * [Cost per Shipping Unit]</expression></dataItem></selection>
		</query><query name="WMS">
			<source>
				
			<sqlQuery dataSource="wms" name="SQL1">
			<sqlText>SELECT whse_code AS &quot;Warehouse&quot;
     , ib_oid AS &quot;PO Number&quot;
	 , host_order_lno AS &quot;PO Line Number&quot;
	 , receipt AS &quot;Receipt&quot;
	 , RIGHT(&apos;00000000&apos; + sku,8) AS &quot;SKU&quot;
	 , qty AS &quot;Quantity&quot;
	 , CAST(BtlPerCase AS INTEGER) AS &quot;Bottles per Case&quot;
	 , CAST(qty / BtlPerCase AS DECIMAL(10,2)) AS &quot;Case Quantity&quot;
	 , transaction_id AS &quot;Transaction ID&quot;
	 , status AS &quot;Order Line Status&quot;
	 , is_extracted AS &quot;Extracted&quot;
	 , create_stamp AS &quot;Creation Date/Time&quot;
	 , mod_stamp AS &quot;Last Update Date/Time&quot;
FROM (
SELECT po.whse_code
     , po.ib_oid
	 , po.host_order_lno
	 , po.receipt
	 , po.sku
	 , qty
	 , CASE WHEN pm_f.uom1 = &apos;BTL&apos; THEN pm_f.qty1in2 ELSE 1 END AS BtlPerCase
	 , po.transaction_id
	 , po.status
	 , po.is_extracted
	 , po.create_stamp
	 , po.mod_stamp
FROM (
(SELECT whse_code
      , ib_oid
	  , host_order_lno
	  , receipt
	  , sku
	  , CASE WHEN pkg = &apos;*&apos; THEN &apos;&apos; ELSE pkg END pkg
	  , cmp_qty AS qty
	  , CASE WHEN receipt = &apos;&apos; AND cmp_qty = 0 THEN &apos;Not Received&apos; ELSE &apos;Order Line Not Closed&apos; END as status
	  , &apos;N/A&apos; AS transaction_id
	  , 0 AS is_extracted
	  , create_stamp
	  , mod_stamp
 FROM   ibod_f
WHERE   ISNUMERIC(ib_oid) = 1
)
UNION ALL
(SELECT up_rcpt.whse_code
      , up_rcpt.ib_oid
	  , up_rcpt_line.host_order_lno
	  , up_rcpt_line.receipt
	  , sku
	  , CASE WHEN up_rcpt_line.pkg = &apos;*&apos; THEN &apos;&apos; ELSE up_rcpt_line.pkg END AS pkg
	  , up_rcpt_line.qty AS qty
	  , CASE WHEN is_extracted = 0 THEN &apos;Order Line Not Interfaced&apos; 
	         WHEN up_rcpt_line.qty = 0 THEN &apos;Order Line Not Interfaced - Zero Quantity&apos;
	         ELSE &apos;Order Line Interfaced&apos; END AS status
	  , CONCAT(up_rcpt.up_rcpt_id,up_rcpt.up_rcpt_up_rid) AS transaction_id
	  , up_rcpt.is_extracted
	  , create_stamp
	  , up_rcpt.mod_stamp
 FROM   v_u_up_rcpt up_rcpt
      , v_u_up_rcpt_line up_rcpt_line
 WHERE  up_rcpt.whse_code = up_rcpt_line.whse_code
   AND  up_rcpt.up_rcpt_id = up_rcpt_line.up_rcpt_id
   AND  up_rcpt.whse_code = up_rcpt_line.whse_code
   AND  ISNUMERIC(up_rcpt.ib_oid) = 1
)
) AS    po
      , pm_f
WHERE   po.whse_code = &apos;VDC&apos;
  AND   po.whse_code = pm_f.whse_code
  AND   po.sku = pm_f.sku
  AND   po.pkg = pm_f.pkg
) AS po_pm_f
ORDER BY whse_code, ib_oid, host_order_lno, receipt, sku

</sqlText>
